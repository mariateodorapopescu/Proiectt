          <ownedParameter xmi:id="_V-xi1EVdEfCS7cth2SLu0w" name="out" type="_T-CaAEVdEfCS7cth2SLu0w"/>
          <language>java</language>
          <body>String context = generateEnhancedContextFromQueryInfo(queryInfo, tokenization);&#xD;
  &#xD;
