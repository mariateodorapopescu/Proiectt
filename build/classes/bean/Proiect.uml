  }&#xD;
  &#xD;
  String message = &quot;IatƒÉ &quot; + (isMinQuery ? &quot;cele mai mici&quot; : &quot;cele mai mari&quot;) + &#xD;
                  &quot; valori pentru &quot; + context + &quot;:&quot;;&#xD;
