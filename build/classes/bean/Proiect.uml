                     } else if (isIncepator) {&#xD;
                         response.sendRedirect(&quot;tip2ok.jsp&quot;);&#xD;
                     } else {&#xD;
                         response.sendRedirect(&quot;dashboard.jsp&quot;);&#xD;
                     }&#xD;
                     return;&#xD;
