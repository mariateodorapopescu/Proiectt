        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_V-yw10VdEfCS7cth2SLu0w" name="executeEnhancedSQLQuery" specification="_V8N99UVdEfCS7cth2SLu0w">
          <ownedParameter xmi:id="_V-yw2EVdEfCS7cth2SLu0w" name="sqlQuery">
