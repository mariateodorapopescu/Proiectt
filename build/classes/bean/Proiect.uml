         case LIST:&#xD;
             sql.append(&quot;SELECT id_sediu, nume_sediu, tip_sediu, strada, oras, judet, tara, &quot;);&#xD;
             sql.append(&quot;telefon, email, data_creare &quot;);&#xD;
