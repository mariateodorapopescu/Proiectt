         // Creare conexiune directƒÉ&#xD;
         conn = DriverManager.getConnection(&quot;jdbc:mysql://localhost:3306/test?useSSL=false&quot;, &quot;root&quot;, &quot;student&quot;);&#xD;
         &#xD;
