                                             &quot;FROM tasks WHERE id = ?&quot;;&#xD;
                     try (PreparedStatement pstmtNotif = connection.prepareStatement(sqlNotification)) {&#xD;
                         pstmtNotif.setString(1, id);&#xD;
                         pstmtNotif.setString(2, id);&#xD;
