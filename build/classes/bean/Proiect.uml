          </ownedParameter>
          <ownedParameter xmi:id="_V-yw2UVdEfCS7cth2SLu0w" name="queryInfo" type="_Vtp9gEVdEfCS7cth2SLu0w"/>
          <ownedParameter xmi:id="_V-yw2kVdEfCS7cth2SLu0w" name="tokenization" type="_V8IeYEVdEfCS7cth2SLu0w"/>
