          <language>java</language>
          <body>String INSERT_USERS_SQL = &quot;UPDATE useri SET nume = ?, prenume = ?, data_nasterii = ?, adresa = ?, email = ?, telefon = ?, username = ?, id_dep = ?, tip = ? WHERE username = ?&quot;;

