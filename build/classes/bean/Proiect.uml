  if (detectedDept != null) {&#xD;
      suggestions.put(&quot;C√¢»õi angaja»õi sunt √Æn departamentul &quot; + detectedDept + &quot;?&quot;);&#xD;
      suggestions.put(&quot;Ce func»õii existƒÉ √Æn departamentul &quot; + detectedDept + &quot;?&quot;);&#xD;
  }&#xD;
  &#xD;
  response.put(&quot;suggestions&quot;, suggestions);&#xD;
  response.put(&quot;confidence_info&quot;, tokenization.getEntityConfidences());&#xD;
  out.println(response.toString());&#xD;
 </body>
