             sql.append(&quot;telefon, email, data_creare &quot;);&#xD;
             sql.append(&quot;FROM sedii &quot;);&#xD;
             &#xD;
             // Order by creation date (newest or oldest first)&#xD;
             if (queryInfo.toString().contains(&quot;vechi&quot;) || &#xD;
