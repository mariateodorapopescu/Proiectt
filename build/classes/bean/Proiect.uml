                     try (ResultSet rs = sediuStatement.executeQuery()) {&#xD;
                         if (rs.next()) {&#xD;
                             JSONObject workplace = new JSONObject();&#xD;
                             &#xD;
                             // AdƒÉugƒÉm informa»õiile despre sediu&#xD;
                             workplace.put(&quot;type&quot;, &quot;sediu&quot;);&#xD;
