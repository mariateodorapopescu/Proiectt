             // 3. VerificƒÉm dacƒÉ existƒÉ loca»õii specifice pentru utilizator&#xD;
             String userLocQuery = &quot;SELECT lu.*, 'user' as type &quot; +&#xD;
                                 &quot;FROM locatii_useri lu &quot; +&#xD;
