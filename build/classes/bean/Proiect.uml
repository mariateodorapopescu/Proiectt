		    (int[])bf_crypt_ciphertext.clone());

		rs.append(&quot;$2&quot;);
		if (minor >= 'a')
			rs.append(minor);
		rs.append(&quot;$&quot;);
		if (rounds &lt; 10)
			rs.append(&quot;0&quot;);
		if (rounds > 30) {
			throw new IllegalArgumentException(
