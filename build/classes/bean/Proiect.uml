	                    &quot;LEFT JOIN tipuri_sporuri s ON u.sporuri = s.id &quot; +
	                    &quot;LEFT JOIN tipuri_penalizari p ON u.penalizari = p.id &quot; +
	                    &quot;WHERE u.activ = 1&quot;;
	        
