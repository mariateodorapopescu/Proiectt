         if (rs.next()) {&#xD;
             json.put(&quot;id_sediu&quot;, rs.getInt(&quot;id_sediu&quot;));&#xD;
             json.put(&quot;nume_sediu&quot;, rs.getString(&quot;nume_sediu&quot;));&#xD;
