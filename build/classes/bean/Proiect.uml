         &#xD;
         if (results.isEmpty()) {&#xD;
             return new ChatResponse(&quot;Nu am gƒÉsit informa»õii despre departamente.&quot;);&#xD;
         }&#xD;
         &#xD;
         return new ChatResponse(&quot;Am gƒÉsit &quot; + results.size() + &quot; departamente. Dori»õi sƒÉ vede»õi lista completƒÉ?&quot;, results);&#xD;
