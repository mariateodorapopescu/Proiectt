            <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
          </ownedParameter>
          <ownedParameter xmi:id="_V-yw30VdEfCS7cth2SLu0w" name="session" type="_T-DoIEVdEfCS7cth2SLu0w"/>
