                         if (rs.getObject(&quot;id_sediu&quot;) != null) {&#xD;
                             homeAddress.put(&quot;id_sediu&quot;, rs.getInt(&quot;id_sediu&quot;));&#xD;
                             homeAddress.put(&quot;nume_sediu&quot;, rs.getString(&quot;nume_sediu&quot;));&#xD;
                         }&#xD;
                         &#xD;
