        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_V--XA0VdEfCS7cth2SLu0w" name="identifyQueryType" specification="_VtgzlEVdEfCS7cth2SLu0w">
          <ownedParameter xmi:id="_V--XBEVdEfCS7cth2SLu0w" name="text">
