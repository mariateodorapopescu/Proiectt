             if (sediuId > 0) {&#xD;
                 String sediuQuery = &quot;SELECT s.*, 'sediu' as type FROM sedii s WHERE s.id_sediu = ?&quot;;&#xD;
                 &#xD;
                 try (PreparedStatement sediuStatement = connection.prepareStatement(sediuQuery)) {&#xD;
                     sediuStatement.setInt(1, sediuId);&#xD;
