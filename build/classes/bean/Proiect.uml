     }&#xD;
     &#xD;
     // Check for project queries&#xD;
     if (query.contains(&quot;proiect&quot;) || query.contains(&quot;proiecte&quot;) || &#xD;
         query.contains(&quot;task&quot;) || query.contains(&quot;taskuri&quot;) || &#xD;
