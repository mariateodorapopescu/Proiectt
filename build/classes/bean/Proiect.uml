        <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_V-w7uEVdEfCS7cth2SLu0w" name="handleUnrecognizedQueryWithSuggestions" specification="_V8JFdUVdEfCS7cth2SLu0w">
